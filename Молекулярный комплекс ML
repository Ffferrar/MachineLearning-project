      "source": [
        "def f1score (data_as_train,factors_list, max_list ,weights_list):\n",
        " \n",
        "  TP=0\n",
        "  FP=0\n",
        "  FN=0\n",
        "  TN=0\n",
        "\n",
        "  \n",
        "  for i in range (len(data_as_train)):  #смотрим количество совпадений ожидания с реальностью\n",
        "      emae = data_as_train[i][1][\"is_real\"]\n",
        "      if emae==\"Yes\" and fu(data_as_train[i][1],factors_list, max_list ,weights_list)==1:\n",
        "        TP+=1\n",
        "      if emae==\"No\" and fu(data_as_train[i][1],factors_list, max_list ,weights_list)==1:\n",
        "        FP+=1\n",
        "      if emae==\"Yes\" and fu(data_as_train[i][1],factors_list, max_list ,weights_list)==0:\n",
        "        FN+=1\n",
        "      if emae==\"No\" and fu(data_as_train[i][1],factors_list, max_list ,weights_list)==0:\n",
        "        TN+=1\n",
        "  return (TP)/(TP+0.5*(FP+FN))#считаем точность функции"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "markdown",
