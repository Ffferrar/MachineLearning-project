        "a=-1000   #ФУНКЦИЯ F\n",
        "def f (factors_of_complex):\n",
        "  \n",
        "\n",
        "    if factors_of_complex[\"E_fst\"] < a:\n",
        "      return 1\n",
        "    else: \n",
        "      return 0\n"
      ],
