      "source": [
        "def accuracy (data_as_pain):\n",
        "  TP=0\n",
        "  FP=0\n",
        "  FN=0\n",
        "  TN=0\n",
        "  for i in range (len(data_as_pain)):\n",
        "      emae = data_as_pain[i][1][\"is_real\"]\n",
        "      if emae==\"Yes\" and f(data_as_pain[i][1])==1:\n",
        "        TP+=1\n",
        "      if emae==\"No\" and f(data_as_pain[i][1])==1:\n",
        "        FP+=1\n",
        "      if emae==\"Yes\" and f(data_as_pain[i][1])==0:\n",
        "        FN+=1\n",
        "      if emae==\"No\" and f(data_as_pain[i][1])==0:\n",
        "        TN+=1\n",
        "  return (TP+TN)/(TP+TN+FP+FN) #считаем точность функции "
      ],
      "execution_count": null,
      "outputs": []
    },
