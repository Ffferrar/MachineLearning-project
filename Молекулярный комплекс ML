      },
      "source": [
        "# Выбор максимального значения \n",
        "# Функция F"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "ViLwV2XzfnZW"
      },
      "source": [
        "def max_factor(data, factor_name): #МАКС ЗНАЧЕНИЕ\n",
        "  max=-1\n",
        "  for i in range (len(data)):\n",
        "    if abs(data[i][1][factor_name])>max:\n",
        "      max=abs(data[i][1][factor_name])\n",
        "      max1=data[i][1][factor_name]\n",
        "  return max1 \n",
        "\n",
        "def fu (factors_of_complex,factors_list, max_list ,weights_list):\n",
        "  [max,max1,max2,max3,max4,max5,max6,max7] = max_list\n",
        "  [a, b, c, d, e, f, g, h, beta] = weights_list\n",
        "  [factor_name,factor_name1,factor_name2,factor_name3,factor_name4,factor_name5,factor_name6,factor_name7] = factors_list\n",
        "  sinusoid = a*(factors_of_complex[factor_name]/max) + b*(factors_of_complex[factor_name1]/max1)+ c*(factors_of_complex[factor_name2]/max2)+ d*(factors_of_complex[factor_name3]/max3)+ e*(factors_of_complex[factor_name4]/max4)+ f*(factors_of_complex[factor_name5]/max5)+ g*(factors_of_complex[factor_name6]/max6)+ h*(factors_of_complex[factor_name7]/max7) +beta\n",
        "  \n",
        "  if 1/(1+math.exp(-1*sinusoid))> 0.5:\n",
        "    return 1\n",
        "  else: \n",
        "    return 0\n"
      ],
      "execution_count": null,
      "outputs": []
    },
