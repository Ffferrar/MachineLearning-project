        "# Bette_weight"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "HYkSGaMsv6s_"
      },
      "source": [
        "def better_weight(current_weights, index_of_better,step):\n",
        "  weight00 = current_weights[index_of_better]\n",
        "  weight1=weight00+step\n",
        "  current_weights[index_of_better] = weight1\n",
        "  F1SCORE1 = f1score(data_as_train, factors_list, max_list ,weights_list)\n",
        "  weight2=weight00-step\n",
        "  current_weights[index_of_better] = weight2\n",
        "  F1SCORE2 = f1score(data_as_train, factors_list, max_list ,weights_list)\n",
        "  if F1SCORE1>=F1SCORE2:\n",
        "    current_weights[index_of_better]=weight1\n",
        "    return current_weights[index_of_better]\n",
        "  else: \n",
        "    current_weights[index_of_better]=weight2\n",
        "    return  current_weights[index_of_better]"
      ],
      "execution_count": null,
      "outputs": []
